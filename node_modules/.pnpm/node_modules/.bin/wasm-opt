#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/postcss-px-to-viewport-wasm-plugin/node_modules/.pnpm/binaryen@112.0.0-nightly.20230411/node_modules/binaryen/bin/node_modules:/mnt/d/project/postcss-px-to-viewport-wasm-plugin/node_modules/.pnpm/binaryen@112.0.0-nightly.20230411/node_modules/binaryen/node_modules:/mnt/d/project/postcss-px-to-viewport-wasm-plugin/node_modules/.pnpm/binaryen@112.0.0-nightly.20230411/node_modules:/mnt/d/project/postcss-px-to-viewport-wasm-plugin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/project/postcss-px-to-viewport-wasm-plugin/node_modules/.pnpm/binaryen@112.0.0-nightly.20230411/node_modules/binaryen/bin/node_modules:/mnt/d/project/postcss-px-to-viewport-wasm-plugin/node_modules/.pnpm/binaryen@112.0.0-nightly.20230411/node_modules/binaryen/node_modules:/mnt/d/project/postcss-px-to-viewport-wasm-plugin/node_modules/.pnpm/binaryen@112.0.0-nightly.20230411/node_modules:/mnt/d/project/postcss-px-to-viewport-wasm-plugin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../binaryen/bin/wasm-opt" "$@"
else
  exec node  "$basedir/../binaryen/bin/wasm-opt" "$@"
fi
